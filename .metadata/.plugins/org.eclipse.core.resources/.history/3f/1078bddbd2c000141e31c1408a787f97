import Global.*;
public class Descriptor {
	
	public Descriptor(){
		
	}
	
	public static void generateExploreMap(){
		int biCount=301;
		System.out.println("11");
		for (int i=0;i<20;i++){
			for (int j=0;j<15;j++) {
				if (Global.exploreMap[i][j]==1){
					System.out.print(1);
				}
				if(Global.exploreMap[i][j]==0){
					System.out.print(0);
				}
				
			}
		}
		
	}

}
